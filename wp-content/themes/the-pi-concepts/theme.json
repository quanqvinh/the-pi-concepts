{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "customTemplates": [],
  "pattern": [],
  "templateParts": [
    {
      "area": "header",
      "name": "header",
      "title": "Header"
    },
    {
      "area": "footer",
      "name": "footer",
      "title": "Footer"
    }
  ],
  "settings": {
    "useRootPaddingAwareAlignments": true,
    "color": {
      "background": true,
      "text": true,
      "link": true,
      "custom": true,
      "customDuotone": true,
      "defaultDuotone": true,
      "defaultGradients": true,
      "defaultPalette": true,
      "palette": [
        {
          "slug": "white",
          "name": "White - #FFFFFF",
          "color": "#FFFFFF"
        },
        {
          "slug": "black",
          "name": "Black - #000000",
          "color": "#000000"
        },
        {
          "slug": "gray-100",
          "name": "Gray 100 - #F9FAFB",
          "color": "#F9FAFB"
        },
        {
          "slug": "gray-200",
          "name": "Gray 200 - #EAECEF",
          "color": "#EAECEF"
        },
        {
          "slug": "gray-400",
          "name": "Gray 400 - #888E9C",
          "color": "#888E9C"
        },
        {
          "slug": "gray-700",
          "name": "Gray 700 - #181E2D",
          "color": "#181E2D"
        },
        {
          "slug": "cyan-100",
          "name": "Cyan 100 - #F9FAFB",
          "color": "#F9FAFB"
        },
        {
          "slug": "cyan-200",
          "name": "Cyan 200 - #B1BDBD",
          "color": "#B1BDBD"
        },
        {
          "slug": "cyan-400",
          "name": "Cyan 400 - #6E8B92",
          "color": "#6E8B92"
        },
        {
          "slug": "cyan-700",
          "name": "Cyan 700 - #265164",
          "color": "#265164"
        },
        {
          "slug": "surface",
          "name": "Surface/Background - #EEEDE6",
          "color": "#EEEDE6"
        }
      ]
    },
    "border": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "custom": {
      "lineHeight": {
        "tight": "1",
        "normal": "1.2",
        "wide": "1.5",
        "wider": "2",
        "fixed-compact": "22px"
      }
    },
    "dimensions": {
      "minHeight": true
    },
    "layout": {
      "contentSize": "77.5rem",
      "wideSize": "100vw",
      "fullSize": "100%"
    },
    "position": {
      "sticky": true
    },
    "spacing": {
      "blockGap": true,
      "margin": true,
      "padding": true,
      "units": ["px", "em", "rem", "vh", "vw", "%"]
    },
    "typography": {
      "customFontSize": true,
      "dropCap": true,
      "fontStyle": true,
      "fontWeight": true,
      "letterSpacing": true,
      "lineHeight": true,
      "textColumns": true,
      "textDecoration": true,
      "textTransform": true,
      "writingMode": true,
      "fontFamilies": [
        {
          "name": "Arsenal (Primary)",
          "slug": "primary",
          "fontFamily": "Arsenal, sans-serif",
          "fontFace": [
            {
              "fontFamily": "Arsenal",
              "fontWeight": "400",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": ["file:./assets/fonts/Arsenal/Arsenal-Regular.ttf"]
            },
            {
              "fontFamily": "Arsenal",
              "fontWeight": "700",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": ["file:./assets/fonts/Arsenal/Arsenal-Bold.ttf"]
            }
          ]
        },
        {
          "name": "System font",
          "slug": "system-font",
          "fontFamily": "system-ui, sans-serif"
        }
      ],
      "fluid": false,
      "fontSizes": [
        {
          "name": "Small",
          "slug": "small",
          "size": "1.25rem"
        },
        {
          "name": "Medium",
          "slug": "medium",
          "size": "1.5625rem"
        },
        {
          "name": "Large",
          "slug": "large",
          "size": "1.875rem"
        },
        {
          "name": "Extra Large",
          "slug": "x-large",
          "size": "3.125rem"
        },
        {
          "name": "Huge",
          "slug": "huge",
          "size": "6.875rem"
        }
      ]
    },
    "blocks": {}
  },
  "styles": {
    "typography": {
      "fontFamily": "var:preset|font-family|primary",
      "fontSize": "var:preset|font-size|medium",
      "lineHeight": "var:custom|line-height|normal"
    },
    "color": {
      "text": "var:preset|color|cyan-700",
      "background": "var:preset|color|surface"
    },
    "border": {
      "radius": "0"
    },
    "blocks": {
      "core/button": {
        "variations": {
          "outline": {
            "color": {
              "text": "var:preset|color|white",
              "background": "transparent"
            },
            "border": {
              "width": "1px",
              "color": "var:preset|color|surface",
              "style": "solid",
              "radius": "0px"
            },
            "css": ".wp-block-button__link:hover {color: var(--wp--preset--color--cyan-700); background: var(--wp--preset--color--surface)}"
          }
        }
      }
    },
    "elements": {
      "heading": {
        "typography": {
          "fontWeight": "700"
        },
        "color": {
          "text": "var:preset|color|gray-700"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var:preset|font-size|huge"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var:preset|font-size|x-large"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var:preset|font-size|x-large"
        }
      },
      "button": {
        "typography": {
          "fontWeight": "700"
        },
        "border": {
          "radius": "0px"
        },
        "spacing": {
          "padding": {
            "top": "1rem",
            "bottom": "1rem",
            "left": "0.5rem",
            "right": "0.5rem"
          }
        },
        "color": {
          "text": "var:preset|color|white",
          "background": "var:preset|color|cyan-700"
        },
        ":hover": {
          "color": {
            "text": "var:preset|color|cyan-400",
            "background": "var:preset|color|surface"
          }
        },
        "css": "transition: all 0.3s; min-width: 150px !important;"
      }
    }
  }
}
